    "numeric-quantity": "^1.0.2",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-justified-grid": "^1.0.0",
    "react-loading": "^2.0.3",
